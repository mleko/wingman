{
    "name": "mleko/wingman",
    "description": "Sort composer.json",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Kr√≥l",
            "email": "daniel@krol.me"
        }
    ],
    "require": {
        "php": "^7.0",
        "composer/composer": "^1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "Mleko\\Wingman\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mleko\\Wingman\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "post-update-cmd": [
            "Mleko\\Wingman\\Composer::format"
        ]
    }
}
